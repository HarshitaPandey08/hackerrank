#include<stdio.h>
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n,k,i,c=0;
        scanf("%d%d",&n,&k);
        int a[n];
        for(i=0;i<n;i++)
        {
            scanf("%d",&a[i]);
        }
        for(i=0;i<n;i++)
        {
            if(a[i]<=0)
            {
                c++;
            }
        }
        if(c>=k)
        {
            printf("NO\n");
        }
        else {
            printf("YES\n");
        }
    }
}
