#include<stdio.h>
int main()
{
    int n,k;
    scanf("%d%d",&n,&k);
    int a[n],i,b,s=0;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    scanf("%d",&b);
    for(i=0;i<n;i++)
    {
        if(i!=k)
        {
            s=s+a[i];
        }
    }
    if((s/2)!=b)
    {
        printf("%d",b-(s/2));

    }
    else
    {
        printf("Bon Appetit");
    }

}
