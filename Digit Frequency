#include<stdio.h>
#include<string.h>
int main()
{
    char s[1000];
    scanf("%s",s);
    int i,n[10]={0};
    for(i=0;i<strlen(s);i++)
    {
        
        if(s[i]>=48 && s[i]<=57)
        {
            n[s[i]-48]++;

        }

    }
    for(i=0;i<10;i++)
    {
        printf("%d ",n[i]);
    }
    
}
