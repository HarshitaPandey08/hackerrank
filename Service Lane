#include<stdio.h>
int main()
{
    int n,t,i,x,y,m;
    scanf("%d%d",&n,&t);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        
    }
    while(t--)
    {
        scanf("%d%d",&x,&y);
        m=a[x];
        for(i=x;i<=y;i++)
        {
            if(a[i]<m)
            {
                m=a[i];
            }

        }
        printf("%d\n",m);
        
    }
}
